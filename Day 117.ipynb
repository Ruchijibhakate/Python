     "text": [
      "          C \n",
      "        O O O \n",
      "      D D D D D \n",
      "    E E E E E E E \n",
      "  R R R R R R R R R \n"
     ]
    }
   ],
   "source": [
    "s='CODER'\n",
    "n=len(s)\n",
    "p=0\n",
    "for i in range(1,n+1):\n",
    "    for j in range(i,n+1):\n",
    "        print(' ',end=' ')\n",
    "    for j in range(1,i+1):\n",
    "        print(s[p],end=' ')\n",
    "    for j in range(2,i+1):\n",
    "        print(s[p],end=' ')\n",
    "    p=p+1\n",
    "    print()"
   ]
