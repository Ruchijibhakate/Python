   "source": [
    "### 1. WAP to skip iteration if the variable i is 3, but continue with next iteration."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "7ff11283-1900-453d-b153-700eec29d1f9",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "0\n",
      "1\n",
      "2\n",
      "4\n",
      "5\n",
      "6\n",
      "7\n",
      "8\n"
     ]
    }
   ],
   "source": [
    "for i in range(9):\n",
    "    if i==3:\n",
    "        continue\n",
    "    print(i)    "
   ]
  },
  {
   "cell_type": "markdown",
   "id": "12409512-be33-48d0-9d25-967672daa066",
   "metadata": {},
   "source": [
    "### 2. Use continue keyword in while loop."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "7c724c4d-30e9-43b1-a7cb-98a6783c41cb",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "1\n",
      "2\n",
      "4\n",
      "5\n",
      "6\n",
      "7\n",
      "8\n",
      "9\n"
     ]
    }
   ],
   "source": [
    "i=0\n",
    "while i<9:\n",
    "    i=i+1\n",
    "    if i==3:\n",
    "        continue\n",
    "    print(i)    "
   ]
  },
