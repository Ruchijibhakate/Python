     "text": [
      "Enter a Number: 123\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Sum of all the digits in number : 6\n"
     ]
    }
   ],
   "source": [
    "num=int(input('Enter a Number:'))\n",
    "sum=0\n",
    "while num>0:\n",
    "    rem=num%10\n",
    "    sum=sum+rem\n",
    "    num=num//10\n",
    "print('Sum of all the digits in number :',sum)    "
   ]
