     "output_type": "stream",
     "text": [
      "Enter a Number : 789\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "987\n"
     ]
    }
   ],
   "source": [
    "num=int(input('Enter a Number :'))\n",
    "res=0\n",
    "while num>0:\n",
    "    rem=num%10\n",
    "    res=res*10+rem\n",
    "    num=num//10\n",
    "\n",
    "print(res)"
   ]
