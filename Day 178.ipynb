   "source": [
    "### Write a Python program to create a histogram from a given list of integers."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "feda1d4c-8928-4a71-904c-3a7057822783",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "**\n",
      "***\n",
      "****\n",
      "*****\n"
     ]
    }
   ],
   "source": [
    "def hist(values):\n",
    "    for n in values:\n",
    "        output=''\n",
    "        times=n\n",
    "        while times>0:\n",
    "            output=output+'*'\n",
    "            times=times-1\n",
    "        print(output)    \n",
    "\n",
    "hist([2,3,4,5])"
   ]
  },
