   "source": [
    "### Extract all odd numbers from a list of integers."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "605c63f8-a35c-4c09-ab60-1fabb7be12e8",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[1, 3, 5, 7, 9]\n"
     ]
    }
   ],
   "source": [
    "lst=[1,2,3,4,5,6,7,8,9]\n",
    "new_lst=list(filter(lambda x : x%2!=0, lst))\n",
    "print(new_lst)"
   ]
