   "source": [
    "### Write a Python program that returns true if the two given integer values are equal or their sum or difference is 5."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "73652b03-a86a-4c4e-8222-8c46a8a1b908",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "None\n",
      "True\n"
     ]
    }
   ],
   "source": [
    "def sum_diff(a,b):\n",
    "    sum=a+b\n",
    "    diff=abs(a-b)\n",
    "    if a==b or sum==5 or diff==5:\n",
    "        return True\n",
    "\n",
    "print(sum_diff(1,2))\n",
    "print(sum_diff(10,5))"
   ]
  },
