   "source": [
    "### Write program to check whether a number is Palindrome or not"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "9c380be6-836f-4ed0-a53c-0dcf52e1879e",
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "Enter a Number : 12321\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Palindrome\n"
     ]
    }
   ],
   "source": [
    "num=int(input('Enter a Number :'))\n",
    "temp=num\n",
    "res=0\n",
    "while temp>0:\n",
    "    rem=temp%10\n",
    "    res=(res*10)+rem\n",
    "    temp=temp//10\n",
    "\n",
    "if res==num:\n",
    "    print(\"Palindrome\")\n",
    "else:\n",
    "    print('Not Palindrome')"
   ]
  },
