   ],
   "source": [
    "s='CODER'\n",
    "n=len(s)\n",
    "p=-1\n",
    "for i in range(n,0,-1):\n",
    "    for j in range(1,i+1):\n",
    "        print(' ',end=' ')\n",
    "    for j in range(i,n+1):\n",
    "        print(s[p],end=' ')\n",
    "    for j in range(i+1,n+1):\n",
    "        print(s[p],end=' ')\n",
    "    p=p-1\n",
    "    print()"
